<div class="container-fluid">
  <h2>Qwerty</h2>

  <% if ruote['workitem'] && ruote['workitem']['fields'] %>
    <%= erb :'partials/_line', :locals => { :ruote => ruote } %>
  <% end %>

  <span>
    <p>JSON format </p>
    <pre><%= JSON.pretty_generate(ruote) %></pre>
  </span>
  <div class="main">
    <footer>
      <p>
      <a href="https://github.com/mozaix/qwerty">Qwerty<a/>, a bot written in Ruby</a>.
      </p>
    </footer>
  </div>
</div>
